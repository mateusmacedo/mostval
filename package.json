{
  "name": "@mostval/source",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mateusmacedo/mostval.git"
  },
  "homepage": "https://github.com/mateusmacedo/mostval#readme",
  "bugs": {
    "url": "https://github.com/mateusmacedo/mostval/issues"
  },
  "scripts": {
    "prepare": "husky",
    "commit": "cz",
    "commit:retry": "git commit --amend --no-edit",
    "commit:check": "commitlint --from HEAD~1 --to HEAD --verbose",
    "commit:validate": "commitlint --from HEAD~1 --to HEAD",
    "lint": "nx affected --target=biome:lint",
    "lint:fix": "nx affected --target=biome:lint-fix",
    "format": "nx affected --target=biome:format",
    "format:fix": "nx affected --target=biome:format-fix",
    "check": "nx affected --target=biome:check",
    "check:fix": "nx affected --target=biome:check-fix",
    "test": "nx affected --target=test",
    "test:go": "nx affected --target=go:test",
    "test:affected": "nx affected --target=test && nx affected --target=go:test",
    "build:go": "nx affected --target=go:build",
    "lint:all": "nx run-many --target=biome:lint",
    "test:all": "nx run-many --target=test",
    "build:all": "nx run-many --target=build",
    "graph": "nx graph",
    "show:projects": "nx show projects",
    "show:affected": "nx show projects --affected",
    "release": "nx release",
    "release:dry-run": "nx release --dry-run",
    "release:version": "nx release --projects=* --version",
    "release:publish": "nx release --projects=* --publish",
    "release:changelog": "nx release --projects=* --changelog",
    "release:affected": "nx release --affected",
    "release:affected:dry-run": "nx release --affected --dry-run",
    "changeset": "changeset",
    "changeset:add": "changeset add",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "changeset:status": "changeset status",
    "release:changeset": "changeset version && changeset publish",
    "release:integrated": "node scripts/release-integrated.js",
    "release:integrated:dry-run": "node scripts/release-integrated.js --dry-run",
    "release:integrated:first": "node scripts/release-integrated.js --first-release",
    "sonar:scan": "nx sonar:scan",
    "sonar:scan:apps": "nx sonar:scan:apps",
    "sonar:scan:libs": "nx sonar:scan:libs"
  },
  "private": false,
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@changesets/cli": "^2.29.7",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@ns3/nx-serverless": "^5.6.0",
    "@nx-go/nx-go": "^3.3.1",
    "@nx/devkit": "20.8.2",
    "@nx/jest": "20.8.2",
    "@nx/js": "20.8.2",
    "@nx/linter": "^19.8.4",
    "@nx/nest": "20.8.2",
    "@nx/node": "20.8.2",
    "@nx/playwright": "20.8.2",
    "@nx/web": "20.8.2",
    "@nx/webpack": "20.8.2",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "^0.2.39",
    "@types/bcryptjs": "^3.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "18.16.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.15.5",
    "commitizen": "^4.3.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.2.0",
    "nx": "20.8.2",
    "sonar-scanner": "3.1.0",
    "swc-loader": "0.1.15",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.7.2",
    "verdaccio": "^6.0.5",
    "webpack-cli": "^5.1.4"
  },
  "config": {
    "commitizen": {
      "path": "./.cz-config.js"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "biome check --write"
    ]
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.2",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/express": "^5.0.3",
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^7.2.2",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "redis": "^5.8.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0",
    "typeorm": "^0.3.27"
  },
  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  }
}
